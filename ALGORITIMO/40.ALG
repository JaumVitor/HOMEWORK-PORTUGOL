algoritmo "40"
var
   CUSTO_FABRICA, DISTRIBUIDOR,IMPOSTOS,CUSTO_CONSUMIDOR: real
inicio
      ESCREVA ("CUSTO DA FABRICA")
      LEIA (CUSTO_FABRICA)
      DISTRIBUIDOR <- (CUSTO_FABRICA * 0.28)
      IMPOSTOS <- (CUSTO_FABRICA * 0.45)
      CUSTO_CONSUMIDOR <- CUSTO_FABRICA + IMPOSTOS

      ESCREVA ("CUSTO DO CONSUMIDOR", CUSTO_CONSUMIDOR)
fimalgoritmo